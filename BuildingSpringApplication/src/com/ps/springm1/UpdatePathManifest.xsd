<?xml version="1.0" encoding="utf-8"?>
<!--
   -  XML Schema for representing information about update path
   -  Last Updated:  2021/02/15
   -  Version 0.1 #2021/02/15#
   -  * First Draft Version.
 
-->
<xs:schema targetNamespace="DellTechnologiesUpdatePathManifest"
    elementFormDefault="qualified"
    xmlns="http://downloads.dell.com/catalog/schema/UpdatePathManifest.xsd"
    xmlns:dtupm="DellTechnologiesUpdatePathManifest"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    version="1.0"
>
  <xs:simpleType name="GUIDType">
    <xs:annotation>
      <xs:documentation>Defines the type for GUID.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}" id="GUID.pattern"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="LocaleStringType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing any string with the locale information</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="locale" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>
              The locale attribute value shall adhere to 3 character as per ISO639-2 standard.
              Reference list is available at http://www.loc.gov/standards/iso639-2/php/code_list.php
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="I18NStringType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing internationalized string</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LocalString" type="dtupm:LocaleStringType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>String in a particular locale</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ProtocolTypeEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the enumeration type for representing the protocol</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="FTP">
        <xs:annotation>
          <xs:documentation>Enumeration representing File Transfer Protocol aka FTP</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TFTP">
        <xs:annotation>
          <xs:documentation>Enumeration representing Trivial File Transfer Protocol aka TFTP</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HTTP">
        <xs:annotation>
          <xs:documentation>Enumeration representing Hypertext Transfer Protocol aka HTTP</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HTTPS">
        <xs:annotation>
          <xs:documentation>Enumeration representing Secure Hypertext Transfer Protocol aka HTTPS</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NFS">
        <xs:annotation>
          <xs:documentation>Enumeration representing Network file share aka NFS</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CIFS">
        <xs:annotation>
          <xs:documentation>Enumeration representing Clustered File system aka CFS</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="HostRepositoryTypeEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing Update Repository Type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="DellDownloads">
        <xs:annotation>
          <xs:documentation>Enumeration representing the regular Dell Repository hosted on downloads.dell.com</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DellRepoMan">
        <xs:annotation>
          <xs:documentation>Enumeration representing the repository created by Dell Repository Manager</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FileSystem">
        <xs:annotation>
          <xs:documentation>Enumeration representing the filesystem</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="YUM">
        <xs:annotation>
          <xs:documentation>Enumeration representing the YUM repository</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="APT">
        <xs:annotation>
          <xs:documentation>Enumeration representing the APT (Advanced Packaging Tool) repository</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="VUM">
        <xs:annotation>
          <xs:documentation>Enumeration representing the VUM repository</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="VMWareDownloads">
        <xs:annotation>
          <xs:documentation>Enumeration representing the repository hosted on https://my.vmware.com/group/vmware/home </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Microsoft">
        <xs:annotation>
          <xs:documentation>Enumeration representing the repository used by Windows Update Service (Mostly https://support.microsoft.com )</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="HostDescriptionType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing a host</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Location" type="xs:string">
        <xs:annotation>
          <xs:documentation>Represents the location of the host</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SupportedProtocols">
        <xs:annotation>
          <xs:documentation>Represents the protocols supported by the host.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:list itemType="dtupm:ProtocolTypeEnumerationType"></xs:list>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="identifier" type="xs:string">
      <xs:annotation>
        <xs:documentation>Represents the unique identifier within the manifest using which the host can be referred in this manifest</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="repositoryType" type="dtupm:HostRepositoryTypeEnumerationType">
      <xs:annotation>
        <xs:documentation>Represents the host repository type</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="requiresAuthentication" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>Represents whether the host requires authentication for access or not.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="recommendedTimeout" type="xs:long">
      <xs:annotation>
        <xs:documentation>Represents the recommended timeout in seconds while accessing this host.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="recommendedRetries" type="xs:long">
      <xs:annotation>
        <xs:documentation>Represents the recommended number of Retries to be done while accessing this host.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="HostedFileInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for describing hosted file.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="hostID" type="xs:string">
      <xs:annotation>
        <xs:documentation>Describes the identifier of the host on which the file is present.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="path" type="xs:string">
      <xs:annotation>
        <xs:documentation>Describes the path at which the file is present.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>Describes the name of the file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="HashAlgorithmType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing the algorithm used for generating hash.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="MD5">
        <xs:annotation>
          <xs:documentation>Enumeration representing MD5.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SHA0">
        <xs:annotation>
          <xs:documentation>Enumeration representing SHA0.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SHA1">
        <xs:annotation>
          <xs:documentation>Enumeration representing SHA1.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SHA256">
        <xs:annotation>
          <xs:documentation>Enumeration representing SHA256.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SHA512">
        <xs:annotation>
          <xs:documentation>Enumeration representing SHA512.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="DigestType">
    <xs:annotation>
      <xs:documentation>Defines the type for describing Digest. Usually for Files or Messages.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="hash" type="xs:hexBinary">
      <xs:annotation>
        <xs:documentation>Represents the hash value of the digest being represented</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="hashAlgorithm" type="dtupm:HashAlgorithmType">
      <xs:annotation>
        <xs:documentation>Represents the algorithm used in calculating the hash.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="SignatureInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing signature verification information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SignFile" type="dtupm:HostedFileInformationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Represents the sign file hosting information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="signed" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>Represents whether the entity is signed or not.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="embeddedSignature" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>Represents whether the entity has embedded the signing information or not.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="publicKeyID" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Represents the public key identifier that is to be used for verifying the signature</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="thumbprint" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Represents the thumbprint of the certificate. Useful for programatic interfaces.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="FileSizeUnitEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing file size unit</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Byte">
        <xs:annotation>
          <xs:documentation>Represents the size in bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="KB">
        <xs:annotation>
          <xs:documentation>Represents the size in Kilo bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MB">
        <xs:annotation>
          <xs:documentation>Represents the size in Mega bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GB">
        <xs:annotation>
          <xs:documentation>Represents the size in Giga bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TB">
        <xs:annotation>
          <xs:documentation>Represents the size in Tera bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PB">
        <xs:annotation>
          <xs:documentation>Represents the size in Peta bytes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="FileInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for describing file information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="HostedFile" type="dtupm:HostedFileInformationType" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Represents the file host information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Digest" type="dtupm:DigestType" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Represents the digest of the file being represented. Using this information one can verify whether the file is transferred correctly or not.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Signature" type="dtupm:SignatureInformationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Represents the signature information for the file being represented. Using this information one can verify the file is authentic or not.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="size" type="xs:float">
      <xs:annotation>
        <xs:documentation>Represents the size of the file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sizeUnit" type="dtupm:FileSizeUnitEnumerationType">
      <xs:annotation>
        <xs:documentation>Represents the file size unit.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="creationDateTime" type="xs:dateTime">
      <xs:annotation>
        <xs:documentation>Represents the creation date time for the file being represented.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="modificationDateTime" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation>Represents the modification date time for the file being represented.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="creator" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Represents the creator information i.e. who or which application created this file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="identifier" type="dtupm:GUIDType">
      <xs:annotation>
        <xs:documentation>Represents the unique identifier for referring to this file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="embeddedSignature" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>Represents whether the file has embedded signature information or not. If it is false then Signature element has to be present.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="HexBinary8Type">
    <xs:annotation>
      <xs:documentation>Defines the type for hexadecimal binary of length 8</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Fa-f]{8}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="HexBinary2Type">
    <xs:annotation>
      <xs:documentation>Defines the type for hexadecimal binary of length 4</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Fa-f]{2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="HexBinary4Type">
    <xs:annotation>
      <xs:documentation>Defines the type for hexadecimal binary of length 4</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Fa-f]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="HexBinary16Type">
    <xs:annotation>
      <xs:documentation>Defines the type for hexadecimal binary of length 16</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Fa-f]{16}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PublicKeyInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for describing public key information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="KeyFile" type="dtupm:FileInformationType">
          <xs:annotation>
            <xs:documentation>Describes the host on which the file is present.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Blob" type="xs:string">
          <xs:annotation>
            <xs:documentation>The key information as binary blob.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
    <xs:attribute name="fingerPrint" type="xs:token">
      <xs:annotation>
        <xs:documentation>Describes the finger print of the public key</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fingerPrintLongKeyID" type="dtupm:HexBinary16Type">
      <xs:annotation>
        <xs:documentation>Describes the long key ID of finger print for the public key being represented.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fingerPrintKeyID" type="dtupm:HexBinary8Type">
      <xs:annotation>
        <xs:documentation>Describes the long key ID of finger print for the public key being represented.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="identifier" type="xs:token">
      <xs:annotation>
        <xs:documentation>Represents the unique identifier for referring to this public key.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="DomicileEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing the domicile type.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Platform">
        <xs:annotation>
          <xs:documentation>Represents platform as domicile type.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Device">
        <xs:annotation>
          <xs:documentation>Represents device as domicile type.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OS">
        <xs:annotation>
          <xs:documentation>Represents os as domicile type.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ManagementController">
        <xs:annotation>
          <xs:documentation>Represents Management Controller as Domicile type</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Application">
        <xs:annotation>
          <xs:documentation>Represents Application as Domicile type essentially for representing application parts</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Firmware">
        <xs:annotation>
          <xs:documentation>Represents Firmware as Domicile type essentially for representing Firmware parts</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="EntityInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing the Domicile or Residence information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="dtupm:I18NStringType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Name of this entity.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="type" type="dtupm:DomicileEnumerationType">
      <xs:annotation>
        <xs:documentation>Represents the type of domicile</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="identifier" type="xs:string">
      <xs:annotation>
        <xs:documentation>Represents the Domicile identifier that remains the same for life.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="LiaisonInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing the liaison information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Entity" type="dtupm:EntityInformationType" minOccurs="2" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>The parties or entities amongst which the liaisoning is to be done.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ChildTypeEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing type of a child.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="PART">
        <xs:annotation>
          <xs:documentation>Part Child i.e. part like DLL for an application etc.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Plugin">
        <xs:annotation>
          <xs:documentation>Plugin Child i.e. plugin for an application etc.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CFG">
        <xs:annotation>
          <xs:documentation>Configuration</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DATA">
        <xs:annotation>
          <xs:documentation>Data</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ParentChildRelationInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing parent relationship.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ParentIdentifier" type="dtupm:EntityInformationType">
        <xs:annotation>
          <xs:documentation>Represents the parent information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChildIdentifier" type="dtupm:EntityInformationType">
        <xs:annotation>
          <xs:documentation>Represents the child identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="childType" type="dtupm:ChildTypeEnumerationType">
      <xs:annotation>
        <xs:documentation>Describes the type of child</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="UpdateOperationContextTypeEnumerationType">
    <xs:annotation>
      <xs:documentation>Defines the enumeration type for update operation context</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Local">
        <xs:annotation>
          <xs:documentation>Represents the local context i.e. the interface/application performing the context is in the same location as the target</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MgmtController">
        <xs:annotation>
          <xs:documentation>Represents the management controller i.e. the update operation is being performed from management controller</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemoteMgmtController">
        <xs:annotation>
          <xs:documentation>Represents the remote management controller i.e. the update operation is being performed from remote management controller</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OS">
        <xs:annotation>
          <xs:documentation>Represents the operating system i.e. the update operation is being performed from operating system of the target system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemoteOS">
        <xs:annotation>
          <xs:documentation>Represents the remote operating system i.e. the update operation is being performed from operating system of a remote system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Container">
        <xs:annotation>
          <xs:documentation>Represents the container i.e. the update operation is being performed from a container hosted on the target system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RemoteContainer">
        <xs:annotation>
          <xs:documentation>Represents the container i.e. the update operation is being performed from a container hosted on a remote system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PublicCloud">
        <xs:annotation>
          <xs:documentation>Represents the public cloud i.e. the update operation is being performed from public cloud</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PrivateCloud">
        <xs:annotation>
          <xs:documentation>Represents the private cloud i.e. the update operation is being performed from private cloud</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OnPremCloud">
        <xs:annotation>
          <xs:documentation>Represents the on premises cloud i.e. the update operation is being performed from on-premises cloud</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="UpdateOperationContextInformationType">
    <xs:annotation>
      <xs:documentation>Defines the type for representing Update Operation Context</xs:documentation>
